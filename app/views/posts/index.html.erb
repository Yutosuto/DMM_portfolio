<head>
       <meta charset="utf-8">
       <meta name="viewport" content="width=device-width,initial-scale=1.0">
        <link rel="stylesheet" type="text/css" href="https://coco-factory.jp/ugokuweb/wp-content/themes/ugokuweb/data/reset.css">
        <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css">
        <link rel="stylesheet" type="text/css" href="https://coco-factory.jp/ugokuweb/wp-content/themes/ugokuweb/data/6-1-7/css/6-1-7.css">
</head>
<body>
       <ul class="slider">
            <li><img src="assets/image1.jpg" alt=""></li>
            <li><img src="assets/image2.jpg" alt=""></li>
            <li><img src="assets/image3.jpg" alt=""></li>
            <li><img src="assets/image4.jpg" alt=""></li>
       </ul>
       <script src="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
       <script src="https://coco-factory.jp/ugokuweb/wp-content/themes/ugokuweb/data/6-1-7/js/6-1-7.js"></script>
</body>

　    <div class="ranktitle">Rankingu</div>
　     <div class="post_rank">
 　        <% @all_ranks.each.with_index(1) do |post, i| %>
            <h5>第<%= i %>位</h5>
              <div id="post_<%= post.id %>" class="post_card" >
                    <div class="postline">
                       <%= attachment_image_tag(post.user, :profile_image, :fill, 50, 50,format: 'jpeg', fallback: "no-image.jpg") %></br>
                       <%= post.user.name %></br>
                       <%= attachment_image_tag post, :image, :size => "150x150", fallback: "" %></br>
                       <%= link_to post.title, post_path(post)%></br>
                       <%= post.body %></br>
                       <div class="post_<%= post.id %>"><%= render "favorites/favorites-btn", post: post %>
                         コメント数: <%= @post.post_comments.count %>
                         <%#= link_to '削除', post_path(post), method: :delete, class:"btn-btn-sm btn-danger" %>
                       </div>
                    </div>
               </div>
          <% end %>
        </div>

<div class="posttitle">PostIndex</div>
    <div class="row">
        <% @posts.each do |post| %>
            <div id="post_<%= post.id %>" class="col-md-2" >
                <div class="postline">
                   <%= attachment_image_tag(post.user, :profile_image, :fill, 50, 50,format: 'jpeg', fallback: "no-image.jpg") %></br>
                   <%= post.user.name %></br>
                   <%= attachment_image_tag post, :image, :size => "150x150", fallback: "" %></br>
                   <%= link_to post.title, post_path(post)%></br>
                   <%= post.body %></br>
                   <div class="post_<%= post.id %>"><%= render "favorites/favorites-btn", post: post %>
                     コメント数: <%= @post.post_comments.count %>
                     <%#= link_to '削除', post_path(post), method: :delete, class:"btn-btn-sm btn-danger" %>
                   </div>
                </div>
            </div>
        <% end %>
    </div>


<footer>
</footer>